fn fact(mut n i32) i32:
  mut result = 1
  while true:
    if n < 2: break 
    result *= n--
  result

fn countOdd(mut n i32) i32:
  mut result = 0
  while n > 0:
    if n-- % 2 == 0: continue 
    ++result
  result